{
  "skill": {
    "id": "web-scraping",
    "version": "1.0.0",
    "enabled": true
  },
  "defaults": {
    "attributes": ["text"],
    "timeout": 30000,
    "retries": 3,
    "userAgent": "Browser4-WebScraper/1.0"
  },
  "selectors": {
    "common": {
      "article": "article, .article, .post",
      "title": "h1, .title, .heading",
      "content": ".content, .article-body, main",
      "author": ".author, .byline, [rel=author]",
      "date": "time, .date, .published",
      "tags": ".tags a, .categories a"
    }
  },
  "rate_limiting": {
    "enabled": true,
    "requests_per_second": 2,
    "burst": 5
  },
  "caching": {
    "enabled": true,
    "ttl_seconds": 300
  }
}
