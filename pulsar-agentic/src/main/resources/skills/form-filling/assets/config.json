{
  "skill": {
    "id": "form-filling",
    "version": "1.0.0",
    "enabled": true
  },
  "defaults": {
    "submit": false,
    "timeout": 30000,
    "wait_after_fill": 500
  },
  "field_mapping": {
    "common_fields": {
      "firstName": ["firstname", "first_name", "fname", "given-name"],
      "lastName": ["lastname", "last_name", "lname", "family-name"],
      "email": ["email", "e-mail", "mail", "email-address"],
      "phone": ["phone", "telephone", "tel", "phone-number"],
      "address": ["address", "street", "street-address"],
      "city": ["city", "town", "locality"],
      "state": ["state", "province", "region"],
      "zip": ["zip", "zipcode", "postal-code", "postcode"],
      "country": ["country", "country-name"]
    }
  },
  "validation": {
    "enabled": true,
    "check_required": true,
    "check_format": true
  },
  "security": {
    "require_https_for_sensitive": true,
    "sensitive_fields": [
      "password",
      "passwd",
      "credit-card",
      "creditcard",
      "cc-number",
      "cvv",
      "ssn",
      "social-security"
    ]
  },
  "retry": {
    "enabled": true,
    "max_attempts": 3,
    "delay_ms": 1000
  }
}
